        <value>h0</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_lat</key>
        <long_name>Latitude of RHO-points</long_name>
        <value>latitude</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_lon</key>
        <long_name>Longitude of RHO-points</long_name>
        <value>longitude</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_salt</key>
        <long_name>Averaged salinity</long_name>
        <value>salt</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_sigma</key>
        <long_name>Ocean sigma coordinate</long_name>
        <value>z</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_temp</key>
        <long_name>Averaged potential temperature</long_name>
        <value>temp</value>
      </parameter>
      <parameter>
        <key>field_var_time</key>
        <long_name>Averaged time since initialization</long_name>
        <value>time</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_u</key>
        <long_name>U-momentum component</long_name>
        <value>uz</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_v</key>
        <long_name>V-momentum component</long_name>
        <value>vz</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>field_var_zeta</key>
        <long_name>Free-surface elevation</long_name>
        <value>xe</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter>
        <key>file_filter</key>
        <long_name>File filter</long_name>
        <value>champs3D*.nc</value>
        <description>Filename filter (accepted metacharacters * and ?)</description>
      </parameter>
      <parameter>
        <key>input_path</key>
        <long_name>Input path</long_name>
        <value>/home/pverley/ichthyop/dev/io/input/mars/</value>
        <format>path</format>
        <description>Folder containing the MARS NetCDF files.</description>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.dataset.Mars3dDataset</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
