<?xml version="1.0" encoding="UTF-8"?>
<icstructure>
  <block type="option">
    <tree_path>Main/Time</tree_path>
    <key>app.time</key>
    <description>Set the time options of the simulation.</description>
    <parameters>
      <parameter type="serial">
        <key>initial_time</key>
        <long_name>Beginning of simulation</long_name>
        <value>year 2010 month 03 day 01 at 00:00</value>
        <format>date</format>
        <tree_path>Tome/General time options</tree_path>
        <description>Set the date of beginning of simulation. Expressed in seconds ellapsed since the origin of time.</description>
      </parameter>
      <parameter type="serial">
        <key>time_step</key>
        <long_name>Time step</long_name>
        <value>300</value>
        <format>integer</format>
        <description>The computational time step (dt) expressed in seconds.</description>
      </parameter>
      <parameter type="serial">
        <key>transport_duration</key>
        <long_name>Transport duration</long_name>
        <value>0000 day(s) 12 hour(s) 00 minute(s)</value>
        <format>duration</format>
        <description>Set the duration [second] of particle transport.</description>
      </parameter>
      <parameter hidden="true">
        <key>calendar_type</key>
        <long_name>Type of calendar</long_name>
        <value>gregorian</value>
        <format>combo</format>
        <accepted>climato</accepted>
        <accepted>gregorian</accepted>
        <tree_path>Tome/General time options</tree_path>
        <description>Choose the type of calendar. Climato: 360 days per year. Gregorian: the usual calendar.</description>
      </parameter>
      <parameter hidden="true">
        <key>time_arrow</key>
        <long_name>Direction of the simulation</long_name>
        <value>forward</value>
        <format>combo</format>
        <accepted>backward</accepted>
        <accepted>forward</accepted>
        <description>Run the simulation backward or forward in time.</description>
      </parameter>
      <parameter hidden="true">
        <key>time_origin</key>
        <long_name>Origin of tile</long_name>
        <value>1900/01/01 00:00</value>
        <description>Origin of time for the Gregorian calendar.</description>
      </parameter>
    </parameters>
  </block>
  <block type="option">
    <key>app.output</key>
    <tree_path>Main/Output</tree_path>
    <parameters>
      <parameter>
        <key>output_path</key>
        <long_name>Output path</long_name>
        <value>output/</value>
        <format>path</format>
        <description>Select the folder where the simulation output files should be saved.</description>
      </parameter>
      <parameter>
        <key>record_frequency</key>
        <long_name>Record frequency</long_name>
        <value>6</value>
        <format>integer</format>
        <description>Set the record frequency as a number of computational time-steps</description>
      </parameter>
      <parameter hidden="true">
        <key>file_prefix</key>
        <long_name>Output filename prefix</long_name>
        <value>irsn-manga2d-opendap</value>
        <description>Type the prefix of the simulation output file. The output filename will look like your-prefix_ichthyop-runYYYYMMDDHHmm.nc</description>
      </parameter>
    </parameters>
  </block>
  <block type="dataset">
    <key>dataset.mars_2d_opendap</key>
    <tree_path>Main/MARS 2D OpenDAP</tree_path>
    <parameters>
      <parameter>
        <key>north-west-corner.lat</key>
        <long_name>Latitude of Northwest corner</long_name>
        <value>50.0</value>
        <description>Set the latitude of Northwest corner for resizing the simulated domain.</description>
        <format>float</format>
        <default>0.0</default>
      </parameter>
      <parameter>
        <key>north-west-corner.lon</key>
        <long_name>Longitude of Northwest corner</long_name>
        <value>-8.0</value>
        <description>Set the longitude of Northwest corner for resizing the simulated domain.</description>
        <format>float</format>
        <default>0.0</default>
      </parameter>
      <parameter>
        <key>opendap_url</key>
        <long_name>OpenDAP URL</long_name>
        <value>http://www.ifremer.fr/thredds3/standard/dodsC/PREVIMER_F1-MARS3D-MANGA_FULL_TIME_SERIE</value>
        <description>Folder containing the MARS NetCDF files.</description>
      </parameter>
      <parameter>
        <key>shrink_domain</key>
        <long_name>Shrink simulated domain</long_name>
        <value>true</value>
        <format>boolean</format>
        <description>Shrink the size of the domain for running the simulation.</description>
      </parameter>
      <parameter>
        <key>south-east-corner.lat</key>
        <long_name>Latitude of Southeast corner</long_name>
        <value>45.0</value>
        <description>Set the latitude of Southeast corner for resizing the simulated domain.</description>
        <format>float</format>
        <default>0.0</default>
      </parameter>
      <parameter>
        <key>south-east-corner.lon</key>
        <long_name>Longitude of Southeast corner</long_name>
        <value>0.0</value>
        <description>Set the longitude of Southeast corner for resizing the simulated domain.</description>
        <format>float</format>
        <default>0.0</default>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.dataset.Mars2dOpendapDataset</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_dim_lat</key>
        <long_name>Dimension in the LAT-direction</long_name>
        <value>latitude</value>
        <description>Set the name of the corresponding NetCDF dimension.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_dim_lon</key>
        <long_name>Dimension in the LON-direction</long_name>
        <value>longitude</value>
        <description>Set the name of the corresponding NetCDF dimension.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_dim_time</key>
        <long_name>Dimension in time</long_name>
        <value>time</value>
        <description>Set the name of the corresponding NetCDF dimension.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_var_bathy</key>
        <long_name>Bathymetry at RHO-points</long_name>
        <value>H0</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_var_lat</key>
        <long_name>Latitude of RHO-points</long_name>
        <value>latitude</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_var_lon</key>
        <long_name>Longitude of RHO-points</long_name>
        <value>longitude</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_var_salt</key>
        <long_name>Averaged salinity</long_name>
        <value>SAL</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_var_temp</key>
        <long_name>Averaged potential temperature</long_name>
        <value>TEMP</value>
      </parameter>
      <parameter hidden="true">
        <key>field_var_time</key>
        <long_name>Averaged time since initialization</long_name>
        <value>time</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_var_u</key>
        <long_name>U-momentum component</long_name>
        <value>U</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
      <parameter hidden="true">
        <key>field_var_v</key>
        <long_name>V-momentum component</long_name>
        <value>V</value>
        <description>Set the name of the corresponding NetCDF variable.</description>
      </parameter>
    </parameters>
  </block>
  <block type="release">
    <key>release.zone</key>
    <tree_path>Main/Zone release</tree_path>
    <enabled>true</enabled>
    <parameters>
      <parameter>
        <key>number_particles</key>
        <long_name>Number of particles</long_name>
        <value>1000</value>
      </parameter>
      <parameter>
        <key>zone_file</key>
        <long_name>Zone file</long_name>
        <value>./cfg/zone-mars2d.xml</value>
        <format>zonefile</format>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.release.ZoneRelease</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.advection</key>
    <tree_path>Advanced/Transport/Advection</tree_path>
    <enabled>true</enabled>
    <parameters>
      <parameter>
        <key>scheme</key>
        <long_name>Numerical scheme</long_name>
        <value>euler</value>
        <format>list</format>
        <accepted>euler</accepted>
        <accepted>rk4</accepted>
        <description>Use a forward Euler scheme (euler) or a Runge-Kutta 4th order scheme (rk4)</description>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.action.AdvectionAction</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <long_name>priority</long_name>
        <key>priority</key>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
        <value>high</value>
        <format>list</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.hdisp</key>
    <tree_path>Advanced/Transport/Horizontal dispersion</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter type="serial">
        <key>epsilon</key>
        <long_name>Dissipation rate</long_name>
        <value>1.0E-9</value>
        <description>Turbulent dissipation rate used in the parametrization of Lagrangian horizontal diffusion.</description>
        <format>float</format>
        <default>1e-9</default>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.action.HDispAction</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>priority</key>
        <long_name>priority</long_name>
        <value>normal</value>
        <format>list</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.vdisp</key>
    <tree_path>Advanced/Transport/Vertical dispersion</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <format>class</format>
        <value>org.previmer.ichthyop.action.VDispAction</value>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>priority</key>
        <long_name>priority</long_name>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
        <value>high</value>
        <format>list</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.buoyancy</key>
    <tree_path>Advanced/Transport/Buoyancy</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter type="serial">
        <key>egg_density</key>
        <long_name>Egg density</long_name>
        <value>1.025</value>
        <format>float</format>
        <default>1.025</default>
        <description>Set the egg density [g/cm3], a long_name parameter to calculate the egg buoyancy.</description>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.action.BuoyancyAction</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>priority</key>
        <long_name>priority</long_name>
        <value>normal</value>
        <format>list</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.migration</key>
    <tree_path>Advanced/Transport/Vertical migration</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter>
        <key>criterion</key>
        <long_name>Migration criterion</long_name>
        <value>Age criterion</value>
        <format>list</format>
        <accepted>Length criterion</accepted>
        <accepted>Age criterion</accepted>
      </parameter>
      <parameter>
        <key>daytime_depth</key>
        <long_name>Depth at daytime</long_name>
        <value>30</value>
      </parameter>
      <parameter>
        <key>limit_age</key>
        <long_name>Limit age</long_name>
        <value>7</value>
      </parameter>
      <parameter>
        <key>limit_length</key>
        <long_name>Limit length</long_name>
        <value>7</value>
      </parameter>
      <parameter>
        <key>nighttime_depth</key>
        <long_name>Depth at nighttime</long_name>
        <value>10</value>
      </parameter>
      <parameter>
        <key>sunrise</key>
        <long_name>Hour of sunrise</long_name>
        <value>7</value>
      </parameter>
      <parameter>
        <key>sunset</key>
        <long_name>Hour of sunset</long_name>
        <value>19</value>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.action.MigrationAction</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>priority</key>
        <long_name>priority</long_name>
        <value>normal</value>
        <format>list</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
  <block type="option">
    <key>release.schedule</key>
    <tree_path>Advanced/Release/Shedule</tree_path>
    <parameters>
      <parameter>
        <key>events</key>
        <long_name>Release event</long_name>
        <value>year 2008 month 01 day 07 at 06:00</value>
        <format>list</format>
      </parameter>
      <parameter>
        <key>is_enabled</key>
        <long_name>Use release schedule</long_name>
        <value>false</value>
        <format>boolean</format>
      </parameter>
    </parameters>
  </block>
  <block type="release">
    <key>release.txtfile</key>
    <tree_path>Advanced/Release/Drifters</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter>
        <key>txtfile</key>
        <long_name>Drifter file</long_name>
        <value>./drifter_example.txt</value>
        <format>file</format>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <format>class</format>
        <value>org.previmer.ichthyop.release.TxtFileRelease</value>
      </parameter>
    </parameters>
  </block>
  <block type="option">
    <key>app.particle_length</key>
    <tree_path>Advanced/Biology/Threshold length</tree_path>
    <parameters>
      <parameter>
        <key>hatch_length</key>
        <long_name>Hatch length</long_name>
        <value>2.8</value>
        <format>float</format>
      </parameter>
      <parameter>
        <key>initial_length</key>
        <long_name>Initial length</long_name>
        <value>0.025</value>
        <format>float</format>
      </parameter>
      <parameter>
        <key>yolk2feeding_length</key>
        <long_name>Yolk to feeding length</long_name>
        <value>4.5</value>
        <format>float</format>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.lethal_temp</key>
    <tree_path>Advanced/Biology/Lethal temperature</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter>
        <key>lethal_temp</key>
        <long_name>Lethal temperature</long_name>
        <value>14</value>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.action.LethalTempAction</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>priority</key>
        <long_name>priority</long_name>
        <value>low</value>
        <format>list</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.growth</key>
    <tree_path>Advanced/Biology/Growth</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter>
        <key>coeff1</key>
        <long_name>Growth coefficient 2</long_name>
        <value>0.03</value>
      </parameter>
      <parameter>
        <key>threshold_temp</key>
        <long_name>Threshold temperature</long_name>
        <value>10</value>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.action.LinearGrowthAction</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>priority</key>
        <long_name>priority</long_name>
        <value>low</value>
        <format>list</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
  <block type="action">
    <key>action.recruitment</key>
    <tree_path>Advanced/Biology/Recruitment</tree_path>
    <enabled>false</enabled>
    <parameters>
      <parameter>
        <key>criterion</key>
        <long_name>Recruitment criterion</long_name>
        <value>age_criterion</value>
      </parameter>
      <parameter>
        <key>duration_min</key>
        <long_name>Minimal duration in recruitment zone</long_name>
        <value>0</value>
      </parameter>
      <parameter>
        <key>limit_age</key>
        <long_name>Reruitment limit age</long_name>
        <value>7</value>
      </parameter>
      <parameter>
        <key>stop_moving</key>
        <long_name>Stop moving when recruited</long_name>
        <value>false</value>
      </parameter>
      <parameter>
        <key>zone_file</key>
        <long_name>Zone file</long_name>
        <value>/home/pverley/ichthyop/dev/nb/iv3/cfg/zone-mars3d.xml</value>
        <format>zonefile</format>
      </parameter>
      <parameter hidden="true">
        <key>class_name</key>
        <long_name>Java Class file</long_name>
        <value>org.previmer.ichthyop.action.RecruitmentAction</value>
        <format>class</format>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
      <parameter hidden="true">
        <key>priority</key>
        <long_name>priority</long_name>
        <value>highest</value>
        <format>combo</format>
        <accepted>lowest</accepted>
        <accepted>low</accepted>
        <accepted>normal</accepted>
        <accepted>high</accepted>
        <accepted>highest</accepted>
        <description>This parameter should not be modified unless you know exactly what you are doing.</description>
      </parameter>
    </parameters>
  </block>
</icstructure>

